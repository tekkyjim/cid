<p>
  <label for="character_name">Name:</label>
  <%= f.text_field :name %>
</p>
<p>
  <label for="character_clan">Clan:</label>
  <%= f.collection_select :clan_id, Clan.all(:order => "name"), :id , :name %>
</p>
<p>
  <label for="character_bio">Bio:</label>
  <%= f.text_field :bio %>
</p>
<p>
  <label for="character_user_id">User:</label>
  <%= f.hidden_field :user_id, :value => current_user.id %> 
</p>
